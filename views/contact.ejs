<script>

    document.getElementById('contact_submit').addEventListener('click',handle_contact_submit);

    function handle_contact_submit(e){

        e.preventDefault();

        
        let contact_email = document.getElementById('contact_email');
        let contact_name = document.getElementById('contact_name');
        let contact_number = document.getElementById('contact_number');
        let contact_message = document.getElementById('contact_message');

        if(contact_email.reportValidity() && contact_name.reportValidity() && contact_number.reportValidity() && contact_message.reportValidity()){

            
            var formData = new FormData();

            formData.append('entry.1378129362',contact_name.value);
            formData.append('entry.19282730',contact_email.value);
            formData.append('entry.1411594711',contact_number.value);
            formData.append('entry.175532735',contact_message.value);


            var requestOptions = {
                method: 'POST',
                body: formData,
                redirect: 'follow',
                mode: "cors"
                };

            fetch("https://docs.google.com/forms/u/0/d/e/1FAIpQLSepzq1zzNAZCjCrlXw_Oz4suINJdhPcA-lBYXmDbr41vcHU9Q/formResponse",requestOptions)
            .then((response)=>{

                alert('send successfully u will recieve confirmation email')
            })
            .catch((err)=>alert('send successfully u will recieve confirmation email'))
            
        }
    }
</script>